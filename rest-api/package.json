{
  "name": "@eneto/rest-api",
  "version": "1.0.0",
  "author": {
    "name": "Ernesto Jara Olveda",
    "email": "ernesto.jara06@gmail.com",
    "url": "https://www.enetojarapro.com"
  },
  "engines": {
    "node": "12.16.1",
    "npm": "6.14.4"
  },
  "license": "Apache-2.0",
  "type": "commonjs",
  "scripts": {
    "lint": "eslint --config ./.eslintrc.js \"./src/*{.ts,.tsx}\" --fix --color",
    "clean": "rm -rf ./dist-babel ./dist-tsc ./api",
    "pretty": "prettier \"./**/**/*.ts\"  \"./**/**/*.js\" --write --color",
    "build:api": "NODE_ENV=production mode=production webpack --config=./config/webpack.config.js --stats --colors",
    "build:ts": "NODE_ENV=production tsc --build",
    "build:babel": "NODE_ENV=production babel --config-file ./babel.config.js ./src --extensions .ts --extensions .tsx --out-dir ./dist-babel",
    "build": "npm run clean && run-p -l build:*",
    "api": "NODE_ENV=development mode=development webpack --config=./config/webpack.config.js --stats --colors",
    "start": "nodemon"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "7.9.2",
    "body-parser": "1.19.0",
    "class-validator": "0.14.0",
    "compression": "1.7.4",
    "core-js": "3.6.5",
    "es6-promise": "4.2.8",
    "express": "4.17.1",
    "helmet": "3.22.0",
    "http-status-codes": "1.4.0",
    "pg": "8.0.2",
    "pg-hstore": "2.3.3",
    "pg-tools": "0.1.0",
    "prettier": "2.0.4",
    "sequelize": "5.21.6",
    "tslib": "1.11.1",
    "tsutils": "3.17.1",
    "typescript": "3.8.3",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/plugin-transform-block-scoping": "7.8.3",
    "@babel/plugin-transform-modules-commonjs": "7.9.0",
    "@babel/plugin-transform-runtime": "7.9.0",
    "@babel/preset-env": "7.9.5",
    "@babel/preset-typescript": "7.9.0",
    "@babel/register": "7.9.0",
    "@types/bluebird": "3.5.30",
    "@types/body-parser": "1.19.0",
    "@types/compression": "1.7.0",
    "@types/cors": "2.8.6",
    "@types/express": "4.17.2",
    "@types/helmet": "0.0.45",
    "@types/node": "13.11.1",
    "@types/swagger-ui-express": "4.1.2",
    "@types/webpack": "4.41.10",
    "@typescript-eslint/eslint-plugin": "2.27.0",
    "@typescript-eslint/parser": "2.27.0",
    "babel-loader": "8.1.0",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "eslint": "6.8.0",
    "eslint-config-jsdoc": "15.4.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-eslint-comments": "3.1.2",
    "eslint-plugin-eslint-plugin": "2.2.1",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsdoc": "22.1.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-promise": "4.2.1",
    "file-loader": "6.0.0",
    "fork-ts-checker-webpack-plugin": "4.1.3",
    "husky": "4.2.5",
    "jsdoc": "3.6.4",
    "nodemon": "2.0.3",
    "npm-run-all": "4.1.5",
    "swagger-ui-express": "4.1.4",
    "ts-node": "8.8.2",
    "tsconfig-paths": "3.9.0",
    "typescript-formatter": "7.2.2",
    "webpack": "4.42.1",
    "webpack-cli": "3.3.11",
    "webpack-node-externals": "1.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run clean && npm run lint",
      "pre-push": "npm run build && npm run lint"
    }
  }
}
